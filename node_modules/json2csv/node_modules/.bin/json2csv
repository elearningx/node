#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/freelance/node-internal/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/node_modules:/mnt/d/freelance/node-internal/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/node_modules:/mnt/d/freelance/node-internal/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules:/mnt/d/freelance/node-internal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/freelance/node-internal/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/bin/node_modules:/mnt/d/freelance/node-internal/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules/json2csv/node_modules:/mnt/d/freelance/node-internal/node_modules/.pnpm/json2csv@6.0.0-alpha.2/node_modules:/mnt/d/freelance/node-internal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/json2csv.js" "$@"
else
  exec node  "$basedir/../../bin/json2csv.js" "$@"
fi
